{
  "name": "ruut-webflow-oauth",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "format": "prettier . --write",
    "clean": "rm -rf dist bundle.zip",
    "build:extension": "mkdir -p dist/webflow-extension && cp -r webflow-extension/* dist/webflow-extension/",
    "bundle": "npm run clean && npm run build:extension && cd dist && zip -r ../bundle.zip . -x \"**/node_modules/*\" \"**/.git/*\" \"*.log\"",
    "deploy:server": "vercel --prod",
    "cy:open": "cypress open"
  },
  "keywords": [],
  "author": "Ruut",
  "license": "ISC",
  "description": "This is a Ruut webflow oauth authorization and authentication server flow",
  "dependencies": {
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "connect-mongodb-session": "^5.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.21.2",
    "express-session": "^1.18.2",
    "form-data": "^4.0.4",
    "helmet": "^8.1.0",
    "jose": "^6.0.12",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.3",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "prettier": "^3.6.2",
    "react-hot-toast": "^2.6.0",
    "react-toastify": "^11.0.5",
    "validator": "^13.15.15",
    "vercel": "^48.4.0",
    "webflow-api": "^3.1.3"
  },
  "devDependencies": {
    "cypress": "^15.3.0"
  }
}
